<script>

    var db = firebase.firestore();

    db.collection("marquee").doc("live")
        .get().then(function (doc) {
            console.log(doc.data().marquee)
            document.getElementById("marqueedata").innerHTML = doc.data().marquee


        })

    var uemail = localStorage.getItem("uemail");
    function checkout() {
        localStorage.removeItem("userid")
        localStorage.removeItem("uemail")
        window.location.replace("/")
    }

    db.collection("users").doc(uemail).get().then((doc) => {
        document.getElementById("username").innerHTML = doc.data().uname
    })


    window.onload = () => {
        var uemail = localStorage.getItem("uemail")
        if (uemail == null) {
            toastr["error"]("Please Login");
            setTimeout(() => { window.location.replace("/login") }, 1000)

        }
    }

    db.collection("users").doc(uemail).collection("wallet").doc(uemail).get()
        .then(function (doc) {
            if (doc.data() != undefined) {
                console.log(doc.data().amount)
                document.getElementById("balance1").innerHTML = doc.data().amount
            }
            else {
                document.getElementById("balance1").innerHTML = 0
            }
        })
</script>