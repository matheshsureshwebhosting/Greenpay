	<script src="/public/js/custom/payment.js"></script>

	<script>

		window.onload = () => {
			var uemail = localStorage.getItem("uemail")
			if (uemail == null) {
				toastr["error"]("Please Login");
				setTimeout(() => { window.location.replace("/login") }, 1000)

			}
		}
	</script>
